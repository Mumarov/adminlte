{
    "name": "jakubsacha/adminlte",
    "description": "AdminLTE theme integration into Syntara & Laravel4",
    "keywords": ["laravel","AdminLTE","theme", "admin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Sacha",
            "email": "jakub.sacha@vert.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
	"mrjuliuss/syntara": "dev-master"
